@model IEnumerable<MyGoParking.Models.MonApplyList>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ApplyStatus)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ApplyDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Car)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Lot)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ApplyStatus)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ApplyDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Car.CarId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Lot.LotId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ApplyId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ApplyId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ApplyId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
